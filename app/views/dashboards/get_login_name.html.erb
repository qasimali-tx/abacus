<div class="container m-top pt-5" style="width: 43rem;">
  <div class="pb-3">
    <h2>Yodlee LogInName</h2>

    <%= form_tag update_yodlee_login_name_dashboards_path, :method => :post, :id => 'update_yodlee_login_name' do %>
      <%= text_field_tag :loginName, "", class: 'form-control' %>
      <div class="button">
        <div class="edit" style="margin-top: 20px;">
          <%= submit_tag "Submit", :id => "create-button", :class => "checkout-button", :style => "top: 0px; font-size: 1.6em;margin-right: 15px;" %>
        </div>
      </div>
    <% end %>
  </div>
</div>